<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.revature.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SubmitServlet</servlet-name>
		<servlet-class>com.revature.controller.SubmitServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ResolvedReimServlet</servlet-name>
		<servlet-class>com.revature.controller.ResolvedReimServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>PendingReimServlet</servlet-name>
		<servlet-class>com.revature.controller.PendingReimServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ViewControllerServlet</servlet-name>
		<servlet-class>com.revature.controller.ViewControllerServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>PendingReimServlet</servlet-name>
		<url-pattern>/spendingreim</url-pattern>
	</servlet-mapping>
	
		<servlet-mapping>
		<servlet-name>ResolvedReimServlet</servlet-name>
		<url-pattern>/sresolvedreim</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/slogin</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SubmitServlet</servlet-name>
		<url-pattern>/ssubmit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewControllerServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewControllerServlet</servlet-name>
		<url-pattern>/employee</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ViewControllerServlet</servlet-name>
		<url-pattern>/submit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewControllerServlet</servlet-name>
		<url-pattern>/pendingreim</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewControllerServlet</servlet-name>
		<url-pattern>/resolvedreim</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewControllerServlet</servlet-name>
		<url-pattern>/manager</url-pattern>
	</servlet-mapping>



</web-app>